#app
  display: flex
  flex-direction: column
  min-height: 100vh
  width: 100%
  .container
    flex-grow: 1
    flex-shrink: 0
    flex-basis: auto
    padding: 2.5rem 1.75rem
    font-family: 'IBM Plex Sans Thai', sans-serif
    .pagination
      color: whitesmoke
    .pagination a
      color: whitesmoke
      text-decoration: underline
    .pagination a:hover
      color: whitesmoke
      text-decoration: none
